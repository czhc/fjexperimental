<div class="col-md-9 col-lg-8">
  <div id="volume">
    <span class="glyphicon glyphicon-volume-up js-volume-toggle " data-value="1"></span>
    <span class="glyphicon glyphicon-volume-off js-volume-toggle hide" data-value="0"></span>
  </div>
  <div class="videoWrappper" id="main-vid">
    <div class="video-container">
      <%= video_tag "", class: 'img-responsive center-block', id: 'main', preload: true %>
    </div>
  </div>
  <div class="sub-vid">
    <div class="col-md-3 col-lg-3 nopadding videoWrapper svchild">
      <div class="video-container">
        <%= video_tag "",class: 'sub', id: '1', preload: true %>
      </div>
    </div>
    <div class="col-md-3 col-lg-3 nopadding videoWrapper svchild">
      <div class="video-container">
        <%= video_tag "",class: 'sub', id: '2', preload: true %>
      </div>
    </div>
    <div class="col-md-3 col-lg-3 nopadding videoWrapper svchild" >
      <div class="video-container">
        <%= video_tag "",class: 'sub', id: '3', preload: true %>
      </div>
    </div>
    <div class="col-md-3 col-lg-3 nopadding videoWrapper svchild">
      <div class="video-container">
        <%= video_tag "",class: 'sub', id: '4', preload: true %>
      </div>
    </div>
  </div>
</div>